;; CONSTANTS
(define cm-per-inch 2.54)
(define inches-per-foot 12)
(define feet-per-yards 3)
(define yards-per-rod 5.5)
(define rods-per-furlong 40)
(define furlongs-per-mile 8)

(define (inches-cm)
  (* (inch 2.54)))

(define (feet-inches)
  (* (feet 12)))

(define (yards-feet)
  (* (yards 3)))

(define (rods-yards)
  (* (rods 5.5)))

(define (furlongs-rods)
  (* (furlongs 40)))

(define (miles-furlongs)
  (* (miles 8)))